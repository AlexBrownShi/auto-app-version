{
  "manifestVersion": 1,
  "id": "thatblokecalledjay-autoappversion",
  "name": "Auto App Version",
  "version": "1.0.8",
  "publisher": "ThatBlokeCalledJay",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/ThatBlokeCalledJay/auto-app-version"
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/ThatBlokeCalledJay/auto-app-version/wiki/Getting-Started"
    },
    "repository": {
      "uri": "https://github.com/ThatBlokeCalledJay/auto-app-version"
    },
    "issues": {
      "uri": "https://github.com/ThatBlokeCalledJay/auto-app-version/issues"
    }
  },
  "scope": [
    "vso.build"
  ],
  "description": "A build and release task for automaticaly incrementing an app's version",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "Auto",
    "Automatic",
    "Build",
    "Version",
    "Increment",
    "Pipeline",
    "Azure DevOps"
  ],
  "content": {
    "license": {
      "path": "license.txt"
    },
    "details": {
      "path": "readme.md"
    }
  },
  "icons": {
    "default": "icon.png"
  },
  "contributions": [
    {
      "id": "AutoAppVersion",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "AutoAppVersion"
      }
    }
  ],
  "files": [
    {
      "path": "AutoAppVersion/task.ps1"
    },
    {
      "path": "AutoAppVersion/task.json"
    },
    {
      "path": "AutoAppVersion/icon.png"
    },
    {
      "path": "AutoAppVersion/ps_modules/vststasksdk"
    }
  ]
}